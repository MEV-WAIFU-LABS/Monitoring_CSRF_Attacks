	FROM phpbb_posts p, phpbb_users u, phpbb_posts_text pt
	WHERE p.topic_id = 6

		AND pt.post_id = p.post_id
		AND u.user_id = p.poster_id
	ORDER BY p.post_time ASC
	LIMIT 0, 15
