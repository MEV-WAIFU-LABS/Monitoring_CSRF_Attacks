	FROM phpbb_topics t, phpbb_users u, phpbb_posts p, phpbb_posts p2, phpbb_users u2
	WHERE t.forum_id = 1
		AND t.topic_poster = u.user_id
		AND p.post_id = t.topic_first_post_id
		AND p2.post_id = t.topic_last_post_id
		AND u2.user_id = p2.poster_id
		AND t.topic_type <> 2

	ORDER BY t.topic_type DESC, t.topic_last_post_id DESC
	LIMIT 0, 50
out mysql_query 1      44 SELECT word, replacement
		FROM  phpbb_words
out sprintf 1       3 %ld
out sprintf 1       3 %ld
out sprintf 1       3 %ld
out fopen 1      26 ./includes/page_header.php
out realpath 1      53 /var/www/html/phpBB2-2.0.5/./includes/page_header.php
out realpath 1      24 ./includes/functions.php
out realpath 1      40 ./templates/subSilver/overall_header.tpl
out sprintf 1       4 %02d
out sprintf 1       2 %d
out sprintf 1       2 %d
out sprintf 1       4 %02d
out sprintf 1       3 %ld
out sprintf 1       3 %ld
out mysql_query 1     285 SELECT u.username, u.user_id, u.user_allow_viewonline, u.user_level, s.session_logged_in, s.session_ip
		FROM phpbb_users u, phpbb_sessions s
		WHERE u.user_id = s.session_user_id
			AND s.session_time >= 1326142009
			AND s.session_page = 1
		ORDER BY u.username ASC, s.session_ip ASC
out sprintf 1       4 %.*f
out sprintf 1       4 %02d
out sprintf 1       2 %d
out sprintf 1       2 %d
out sprintf 1       4 %02d
out sprintf 1       4 %02d
out sprintf 1       2 %d
out sprintf 1       2 %d
out sprintf 1       4 %02d
out fopen 1      65 /var/www/html/phpBB2-2.0.5/templates/subSilver/overall_header.tpl
out sprintf 1      11 %s(%d) : %s
out sprintf 1       3 %ld
out realpath 1      24 ./includes/functions.php
out realpath 1      40 ./templates/subSilver/viewforum_body.tpl
out mysql_query 1     180 SELECT c.cat_id, c.cat_title, c.cat_order
		FROM phpbb_categories c, phpbb_forums f
		WHERE f.cat_id = c.cat_id
		GROUP BY c.cat_id, c.cat_title, c.cat_order
		ORDER BY c.cat_order
out mysql_query 1      61 SELECT *
			FROM phpbb_forums
			ORDER BY cat_id, forum_order
out realpath 1      24 ./includes/functions.php
out realpath 1      33 ./templates/subSilver/jumpbox.tpl
